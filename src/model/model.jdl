entity Environment {
   name String
   description String maxlength(5000)
   createdOn Instant
   updatedOn Instant
   updatedBy String
   createdBy String   
   status String
   clientId String maxlength(1000)
   clientSecret String maxlength(2000)
   userType String
   email String
   password String
   jsonData Blob
   scopes String
   authUrl String
   tokenUrl String
   apiUrl String
   type String
}

entity Input {
   name String
   description String maxlength(5000)
   createdOn Instant
   updatedOn Instant 
   status String
   source String
   type String
   url String
   userId String
   password String
   database String
   inputData Blob
   access String
   basicAuth String
   basicAuthUser String
   basicAuthPassword String
   withCredentials String
   secureInputData Blob
   daprPubSubName String
   daprTopic String
   daprRoute String
   createdBy String
   updatedBy String
}

entity Dashboard {
   name String
   description String maxlength(5000)
   createdOn Instant
   updatedOn Instant 
   status String
   isMonitored  String maxlength(3)
   jsonData Blob
   createdBy String
   updatedBy String
}

entity Organization {
   name String
   description String maxlength(5000)
   createdOn Instant
   updatedOn Instant 
   status String
   createdBy String
   updatedBy String
}

entity OrganizationalUnit {
   name String
   description String maxlength(5000)
   createdOn Instant
   updatedOn Instant 
   status String
   createdBy String
   updatedBy String
}

entity OuEnvMapping {
   
}


relationship ManyToOne {
	Organization{environment} to Environment,
	OrganizationalUnit{organization} to Organization,
	Input{environment} to Environment,
	Dashboard{input} to Input,
	OuEnvMapping{organizationalUnit} to OrganizationalUnit,
	OuEnvMapping{environment} to Environment
}

dto * with mapstruct service all with serviceImpl
